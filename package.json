{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy medic-trace",
        "canister_deploy_mainnet": "dfx deploy --network ic medic-trace",
        "canister_uninstall": "dfx canister uninstall-code medic-trace",
        "canister_call_get_message": "dfx canister call medic-trace getMessage",
        "canister_call_set_message": "dfx canister call medic-trace setMessage '(\"Hello world!\")'"
    },
    "dependencies": {
        "azle": "0.17.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/uuid": "^9.0.5"
    }
}
